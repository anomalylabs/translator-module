{% macro macro(keys) %}
    {% import _self as do %}
    {% for key, value in keys %}
        {% if value is iterable %}
            '{{ key }}' => [
            {{ do.macro(value) }}    ],
        {% else %}
            '{{ key }}' => '{{ value|e }}',
        {% endif %}
    {% endfor %}
{% endmacro %}

{% import _self as do %}

return [
{{ do.macro(require) }}];
